{
  "manifest_version": 2,
  "name": "Feature flags",
  "short_name": "Feature flags",
  "description": "",
  "version": "0.0.3",
  "minimum_chrome_version": "38",

  "icons": {
    "16": "assets/icon-16.png",
    "128": "assets/icon-128.png"
  },

  "browser_action": {
    "default_popup": "index.html",
    "default_title": "Feature flags"
  },
  
  "permissions": [
    "activeTab",
    "cookies",
    "background",
    "http://*/*",
    "https://*/*"
  ]
}
